node {
   stage('init') {
      checkout scm
   }
   
    stage('build and publish') {
        bat 'dotnet publish TestProject.sln -c Release'
    }
   stage('deploy') {
      azureWebAppPublish azureCredentialsId: env.AZURE_CRED_ID,
      resourceGroup: env.RES_GROUP, appName: env.WEB_APP, filePath: "TestProject\bin\Release\netcoreapp2.1\publish\"
   }
}